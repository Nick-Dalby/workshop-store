{
  "extends": ["wesbos"],
  "rules": {
    "camelcase": 0,
    "react/prop-types": 0,
    "no-shadow": 0,
    "react/jsx-no-constructed-context-values": 0,
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2,
        "semi": false
      }
    ]
  }
}
